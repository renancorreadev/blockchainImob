{
  "name": "cleanweb3vite",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "generate": "typechain --target=ethers-v5 './abi/*.json'",
    "prepare": "cd .. && husky install frontend/.husky",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "node --no-warnings --experimental-vm-modules --es-module-specifier-resolution=node --loader ./custom-loader.mjs node_modules/jest/bin/jest.js",
    "test:watch": "yarn jest --watch",
    "release": "standard-version"
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test --passWithNoTests"
    ]
  },
  "dependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.19.6",
    "@celo/rainbowkit-celo": "^0.4.0",
    "@fontsource/archivo": "^4.5.10",
    "@fontsource/audiowide": "^4.5.9",
    "@fontsource/poppins": "^4.5.10",
    "@rainbow-me/rainbowkit": "^0.8.0",
    "@types/canvas-confetti": "^1.6.0",
    "@types/react-modal": "^3.13.1",
    "@vitejs/plugin-react": "^3.0.0",
    "add": "^2.0.6",
    "canvas-confetti": "^1.6.0",
    "ethers": "^5.7.2",
    "jest-resolve": "^29.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-modal": "^3.16.1",
    "react-router-dom": "^6.4.5",
    "react-spinners": "^0.13.7",
    "styled-components": "^5.3.6",
    "swiper": "^8.4.5",
    "wagmi": "^0.8.10",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@honkhonk/vite-plugin-svgr": "^1.1.0",
    "@preact/preset-vite": "^2.3.0",
    "@swc/core": "^1.3.23",
    "@swc/jest": "^0.2.24",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/jest": "^28.1.4",
    "@types/node": "^17.0.14",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/styled-components": "^5.1.25",
    "@types/typescript": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "babel-jest": "^29.3.1",
    "babel-loader": "^8.2.5",
    "babel-plugin-styled-components": "^2.0.7",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^11.0.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.2.0",
    "git-commit-msg-linter": "^4.4.0",
    "husky": "^8.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^28.1.2",
    "lint-staged": "^13.1.0",
    "release": "^6.3.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typedoc": "^0.23.21",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vite-tsconfig-paths": "^4.0.1",
    "vitest": "^0.25.8"
  },
  "aliases": {
    "@*": "./src/*",
    "@assets": "./src/assets/*",
    "@presentation/*": "src/presentation/*",
    "@presentation": "src/presentation/",
    "@data/*": "src/data/*",
    "@data": "src/data/",
    "@main/*": "src/main/*",
    "@main": "src/main/",
    "@infra/*": "src/infra/*",
    "@infra": "src/infra/",
    "@test/*": "./test/*",
    "@pages/*": "./src/presentation/pages/*",
    "@layout": "./src/main/factories/pages/layout",
    "@types/*": "./src/@types/*",
    "@contracts": "src/@types/contracts/*",
    "@utils": "src/utils/",
    "@utils/": "src/utils/*",
    "@utils/ContractConfigs": "./src/utils/ContractConfigs.ts",
    "@Callbacks": "src/infra/callbacks/",
    "@Callbacks/": "src/infra/callbacks/*",
    "@hooks": "./src/infra/hooks/*",
    "@hooks/": "./src/infra/hooks/"
  }
}
