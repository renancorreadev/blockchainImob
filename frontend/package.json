{
  "name": "cleanweb3vite",
  "private": true,
  "version": "1.1.0",
  "description": "Clean Web3 Vite",
  "type": "module",
  "scripts": {
    "generate": "typechain --target=ethers-v5 './abi/*.json'",
    "prepare": "cd .. && husky install frontend/.husky",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest --passWithNoTests --reporter verbose",
    "test:isolate": "yarn test --isolate ",
    "test:watch": "yarn test watch",
    "test:staged": "yarn test related",
    "test:ci": "yarn test --coverage",
    "release": "standard-version",
    "docs": "typedoc --entryPointStrategy  expand --tsconfig ./tsconfig.json --excludeInternal --excludeProtected",
    "docs-hierarchy": "typedoc --entryPointStrategy  expand --tsconfig ./tsconfig.json --plugin ./node_modules/typedoc-theme-hierarchy/dist/index.js --theme hierarchy",
    "docs-github": "typedoc --theme github-wiki --entryPointStrategy  expand --tsconfig ./tsconfig.json  "
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test --passWithNoTests"
    ]
  },
  "dependencies": {
    "@celo/rainbowkit-celo": "^0.4.0",
    "@fontsource/archivo": "^4.5.10",
    "@fontsource/audiowide": "^4.5.9",
    "@fontsource/poppins": "^4.5.10",
    "@rainbow-me/rainbowkit": "^0.8.0",
    "@vitejs/plugin-react": "^3.0.0",
    "canvas-confetti": "^1.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-modal": "^3.16.1",
    "react-router-dom": "^6.4.5",
    "react-spinners": "^0.13.7",
    "styled-components": "^5.3.6",
    "swiper": "^8.4.5",
    "wagmi": "^0.8.10"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@honkhonk/vite-plugin-svgr": "^1.1.0",
    "@playwright/test": "^1.25.0",
    "@preact/preset-vite": "^2.3.0",
    "@rsksmart/mock-web3-provider": "^1.0.1",
    "@swc/core": "^1.3.23",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.2.1",
    "@typechain/ethers-v5": "^9.0.0",
    "@types/canvas-confetti": "^1.6.0",
    "@types/cypress": "^1.1.3",
    "@types/node": "17.0.42",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "@types/react-modal": "^3.13.1",
    "@types/styled-components": "^5.1.25",
    "@types/typescript": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "@vitest/coverage-c8": "^0.25.8",
    "@vitest/ui": "^0.25.8",
    "babel-loader": "^8.2.5",
    "babel-plugin-styled-components": "^2.0.7",
    "dotenv": "^16.0.1",
    "eslint": "8.17.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "ethers": "^5.5.3",
    "git-commit-msg-linter": "^4.4.0",
    "hardhat": "^2.9.2",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^20.0.3",
    "lint-staged": "^13.0.1",
    "prettier": "^2.7.0",
    "pretty-quick": "^3.1.3",
    "release": "^6.3.0",
    "standard-version": "^9.5.0",
    "start-server-and-test": "^1.14.0",
    "ts-node": "^10.8.2",
    "ts-node-dev": "^2.0.0",
    "typechain": "^7.0.0",
    "typedoc": "^0.22.17",
    "typedoc-github-wiki-theme": "^1.0.1",
    "typedoc-plugin-markdown": "^3.14.0",
    "typedoc-plugin-merge-modules": "^3.1.0",
    "typedoc-plugin-missing-exports": "^0.22.6",
    "typedoc-theme-hierarchy": "^1.2.1",
    "typescript": "4.7.3",
    "vi-fetch": "^0.8.0",
    "vite": "^3.2.3",
    "vite-tsconfig-paths": "^4.0.1",
    "vitest": "^0.25.8"
  }
}
